        "sae_batch_size": {
          "default": 125,
          "description": "",
          "title": "SAE Batch Size",
          "type": "integer"
        },
        "llm_batch_size": {
          "default": 32,
          "description": "",
          "title": "LLM Batch Size",
          "type": "integer"

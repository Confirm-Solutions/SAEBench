      "properties": {
        "random_seed": {
          "default": 42,
          "description": "NOTE: This will be overwritten by argparse",
          "title": "Random Seed",
          "type": "integer"
        },
        "dataset_names": {
          "description": "List of dataset names",
          "items": {
            "type": "string"
          },
          "title": "Dataset Names",
